/*Accept the percentage from user and check grade.
a.Above 70%-Grade A
b.Between 60 ton70%- Grade B
c.Between 45 to 60%-Grade C
d.Between 35 to 45%-Grade D
e.Less than 35%-Fail */

#include<stdio.h>
int main()
{
int per;
printf("Enter the percentage:");
scanf("%d",&per);

if(per>=70 && per<=100)
printf("Grade is A");
else if(per>=60 && per<70)
printf("Grade is B+");
else if(per>=45 && per<60)
printf("Grade is B");
else if(per>=35 && per<45)
printf("Grade is C");
else
printf("Fail");

return 0;
}

