//Accept the three integer number from user and find out greatest among them using conditional operator.

#include<stdio.h>
int main()
{
int a,b,c,big;
printf("Enter the number:");
scanf("%d %d %d",&a,&b,&c);
big=(a>b && a>c ? a : b>a && b>c ?b : c);
printf("Greatest number is:%d",big);
return 0;
}
