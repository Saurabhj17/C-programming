//Accept the five subject marks and find out total and average of the marks.

#include<stdio.h>
int main()
{
int eng,mar,math,phys,chem;
float total, avg;

printf("Enter the marks of english:\n");
scanf("%d",&eng);
printf("Enter the marks of marathi:\n");
scanf("%d",&mar);
printf("Enter the marks of math:\n");
scanf("%d",&math);
printf("Enter the marks of physics:\n");
scanf("%d",&phys);
printf("Enter the marks of chemistry:\n");
scanf("%d",&chem);

total=eng+mar+math+phys+chem;
avg=total/5;

printf("Total of all subject is:%f \n",total);
printf("Average of all subject is:%f \n",avg);

return 0;
}
